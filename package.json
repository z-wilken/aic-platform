{
  "name": "aic-platform-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npx concurrently \"npm run dev:web\" \"npm run dev:platform\" \"npm run dev:admin\"",
    "dev:web": "cd apps/web && npm run dev",
    "dev:platform": "cd apps/platform && npm run dev -- -p 3001",
    "dev:admin": "cd apps/admin && npm run dev -- -p 3002",
    "build": "npx concurrently \"cd apps/web && npm run build\" \"cd apps/platform && npm run build\" \"cd apps/admin && npm run build\"",
    "start": "npx concurrently \"cd apps/web && npm run start\" \"cd apps/platform && npm run start\" \"cd apps/admin && npm run start\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:web": "vitest run --project web",
    "test:platform": "vitest run --project platform"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.3.0",
    "concurrently": "^8.2.0",
    "jsdom": "^24.0.0",
    "vitest": "^1.3.0"
  },
  "dependencies": {
    "@types/pg": "^8.16.0",
    "next-auth": "^4.24.13",
    "pg": "^8.18.0"
  }
}
